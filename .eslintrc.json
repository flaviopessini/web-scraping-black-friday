{
    "root": true,
    "env": {
        "es2021": true,
        "node": true,
        "browser": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "camelcase": "warn",
        "default-case": "error",
        "no-empty": "warn",
        "no-empty-function": "warn",
        "no-return-await": "error",
        "prefer-const": "warn",
        "arrow-parens": ["error", "always"]
    }
}
